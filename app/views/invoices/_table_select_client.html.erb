<table class="table table-striped" id='clients_table'>
  <thead>
    <tr>
      <th>Identificacion</th>
      <th>Nombre</th>
      <th>Direccion</th>
      <th colspan="3"></th>
    </tr>
  </thead>


  <tbody>
    <% @clients.each do |client| %>
    <tr>
      <td><%= client.identification %></td>
      <td><%= client.name %></td>
      <td><%= client.direction %></td>
      <td><%= link_to 'ver', client %></td>
    </tr>
    <% end %>
  </tbody>
</table>