<p id="notice"><%= notice %></p>

<h1>Listing State Products</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @state_products.each do |state_product| %>
      <tr>
        <td><%= state_product.name %></td>
        <td><%= link_to 'Show', state_product %></td>
        <td><%= link_to 'Edit', edit_state_product_path(state_product) %></td>
        <td><%= link_to 'Destroy', state_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New State product', new_state_product_path %>
