<p id="notice"><%= notice %></p>

<h1>Listing Invoice Details</h1>

<table>
  <thead>
    <tr>
      <th>Invoice</th>
      <th>Article</th>
      <th>Quantity</th>
      <th>Unit value</th>
      <th>State product</th>
      <th>Observation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @invoice_details.each do |invoice_detail| %>
      <tr>
        <td><%= invoice_detail.invoice %></td>
        <td><%= invoice_detail.article %></td>
        <td><%= invoice_detail.quantity %></td>
        <td><%= invoice_detail.unit_value %></td>
        <td><%= invoice_detail.state_product %></td>
        <td><%= invoice_detail.observation %></td>
        <td><%= link_to 'Show', invoice_detail %></td>
        <td><%= link_to 'Edit', edit_invoice_detail_path(invoice_detail) %></td>
        <td><%= link_to 'Destroy', invoice_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

